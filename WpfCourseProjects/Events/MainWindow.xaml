<Window x:Class="Events.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Events"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Name="grid" Background="Aqua">
         <Border Name="border" Background="Red" Padding="10" Margin="20">
             <StackPanel Name="sp" Background="Blue">
                 <Ellipse Name="el" Width="100" Height="200" Fill="Coral" />
             </StackPanel>
         </Border>
    </Grid>
</Window>
