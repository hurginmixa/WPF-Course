<Window x:Class="Doc_Grid_Poly_Panels.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Doc_Grid_Poly_Panels"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel Name="OuterDocPanel" LastChildFill="True">
              <Rectangle DockPanel.Dock="Left" Width="100" Fill="Aqua"/>
              <Rectangle DockPanel.Dock="Bottom" Height="100" Fill="Red"/>
    </DockPanel>
</Window>
